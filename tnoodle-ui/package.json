{
  "name": "tnoodle-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://localhost:2014/scramble/",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.5",
    "cross-env": "^5.2.0",
    "form-urlencoded": "^2.0.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.2",
    "pluralize": "^4.0.0",
    "react": "16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.0.17",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "redux-mock-store": "^1.3.0",
    "xhr-mock": "^1.9.1"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "cross-env NODE_PATH=./src react-scripts start",
    "start": "cross-env PORT=3001 BROWSER=none npm-run-all -p watch-css start-js",
    "build": "yarn run build-css && cross-env CI=true NODE_PATH=./src react-scripts build",
    "test": "cross-env NODE_PATH=./src react-scripts test --env=jsdom",
    "test:debug": "cross-env NODE_PATH=./src react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "cross-env NODE_PATH=./src react-scripts eject"
  }
}
