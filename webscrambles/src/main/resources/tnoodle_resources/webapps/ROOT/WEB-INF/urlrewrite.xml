<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <rule>
        <from>^/$</from>
        <to type="redirect" last="true" qsappend="true">/scramble/</to>
    </rule>

    <rule>
        <from>^/favicon.ico$</from>
        <to type="redirect" last="true">/wca/favicon.ico</to>
    </rule>

    <rule>
        <from>^/scramble$</from>
        <to type="redirect" last="true" qsappend="true">/scramble/</to>
    </rule>
    <rule>
        <from>^/scramble/static</from>
        <to>/wca/new-ui/static</to>
    </rule>
    <rule>
        <from>^/scramble/[^.]*$</from>
        <to>/wca/new-ui/index.html</to>
    </rule>

    <rule>
        <from>^/scramble-legacy$</from>
        <to type="redirect" last="true" qsappend="true">/scramble-legacy/</to>
    </rule>
    <rule>
        <from>^/scramble-legacy/$</from>
        <to>/wca/scramblegen.html</to>
    </rule>

    <rule>
        <from>^/readme$</from>
        <to>/wca/readme-tnoodle.md</to>
    </rule>
    <rule>
        <from>^/readme.md$</from>
        <to>/wca/readme-tnoodle.md</to>
    </rule>

</urlrewrite>
